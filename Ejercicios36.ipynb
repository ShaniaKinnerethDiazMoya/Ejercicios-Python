   "source": [
    "### Pide un caracter y determina si es una vocal "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "ad8c6e84-df70-419d-8b6c-ef218a64d1d6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Ingresa caracter e\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Es una vocal\n"
     ]
    }
   ],
   "source": [
    "caracter = input(\"Ingresa caracter\")\n",
    "vocales = ['a','e','i','o','u']\n",
    "\n",
    "if caracter.lower() in vocales:\n",
    "    print (\"Es una vocal\") \n",
    "else:\n",
    "    print (\"No es una vocal\")"
   ]
