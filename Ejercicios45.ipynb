   "source": [
    "### Imprime la tabla de multiplicar de un numero ingresado por el usuario"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "a2ccdba8-a80b-4d90-8834-ff393cd066e7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Ingresa el numero 5\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5 * 1 = 5\n",
      "5 * 2 = 10\n",
      "5 * 3 = 15\n",
      "5 * 4 = 20\n",
      "5 * 5 = 25\n",
      "5 * 6 = 30\n",
      "5 * 7 = 35\n",
      "5 * 8 = 40\n",
      "5 * 9 = 45\n",
      "5 * 10 = 50\n"
     ]
    }
   ],
   "source": [
    "numero = int(input(\"Ingresa el numero\"))\n",
    "i = 1\n",
    "while i <= 10:\n",
    "    print(f\"{numero} * {i} = {numero * i}\")\n",
    "    i = i + 1"
   ]
