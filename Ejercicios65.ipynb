     "text": [
      "86.0\n"
     ]
    }
   ],
   "source": [
    "def celsius_a_fahrenheit(grados_c):\n",
    "    return(grados_c * 9/5) + 32\n",
    "\n",
    "resultado = celsius_a_fahrenheit(30)\n",
    "\n",
    "print(resultado)"
   ]
