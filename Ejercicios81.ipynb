   "source": [
    "### Elevar al cuadrado una lista de numeros utilizando map"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "67f658ae-348b-4fe6-b501-79d3c3eef831",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 3, 4, 5]\n",
      "[1, 4, 9, 16, 25]\n"
     ]
    }
   ],
   "source": [
    "def cuadrado(x):\n",
    "    return x ** 2\n",
    "\n",
    "numeros = [1,2,3,4,5]\n",
    "cuadrados = list(map(cuadrado, numeros))\n",
    "\n",
    "print (numeros)\n",
    "print (cuadrados)"
   ]
