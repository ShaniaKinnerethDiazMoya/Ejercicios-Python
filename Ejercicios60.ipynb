   "source": [
    "### Imprimir la suma de los numeros pares del 1 al 10 utilizando el ciclo for "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "1f4c04d0-9089-42a0-b0be-893c1bdea2bf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "suma de pares es: 2\n",
      "suma de pares es: 6\n",
      "suma de pares es: 12\n",
      "suma de pares es: 20\n",
      "suma de pares es: 30\n"
     ]
    }
   ],
   "source": [
    "suma_pares = 0\n",
    "\n",
    "for i in range (1, 11):\n",
    "    if i % 2 == 0:\n",
    "        suma_pares = suma_pares + i\n",
    "        print(\"suma de pares es:\", suma_pares)"
   ]
