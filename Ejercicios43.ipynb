   "source": [
    "### Solicita al usuario ingresar un numero n e imprimir el factorial de ese numero "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "bd98e5d2-de04-475c-97bc-01a014c1599f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Ingresa el numero 5\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "el factorial es:  120\n"
     ]
    }
   ],
   "source": [
    "numero = int(input('Ingresa el numero'))\n",
    "factorial = 1\n",
    "i = 1\n",
    "while i <= numero:\n",
    "    factorial = factorial * i\n",
    "    i = i + 1\n",
    "print('el factorial es: ',factorial)"
   ]
  },
