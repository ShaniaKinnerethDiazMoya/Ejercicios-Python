     "output_type": "stream",
     "text": [
      "ingrese el numero 456\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Digitos son:  3\n"
     ]
    }
   ],
   "source": [
    "numero = int(input('ingrese el numero'))\n",
    "contador = 0\n",
    "while numero != 0:\n",
    "    numero = numero // 10\n",
    "    contador = contador + 1\n",
    "print(\"Digitos son: \", contador)"
   ]
