   "source": [
    "### Obtener la cantidad de memoria ram "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "db51d271-d2ff-4792-8b67-5116132d6022",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Total de ram  15.28499984741211 Gb\n"
     ]
    }
   ],
   "source": [
    "import psutil \n",
    "\n",
    "def obtener_ram():\n",
    "    memoria = psutil.virtual_memory()\n",
    "    memoria_total = memoria.total / (1024 ** 3)\n",
    "    return memoria_total\n",
    "\n",
    "memoria = obtener_ram()\n",
    "\n",
    "print('Total de ram ', memoria, 'Gb')"
   ]
