   "source": [
    "### Convierte una lista de cadenas que sean numeros a enteros utilizando map "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "ed7d103a-ada4-431d-8eb6-6a78160f03b1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['1', '2', '3', '4']\n",
      "[1, 2, 3, 4]\n"
     ]
    }
   ],
   "source": [
    "def convertir_entero(cadena):\n",
    "    return int(cadena)\n",
    "\n",
    "cadenas = [\"1\",\"2\",\"3\",\"4\"]\n",
    "\n",
    "enteros = list(map(convertir_entero, cadenas))\n",
    "\n",
    "print(cadenas)\n",
    "print(enteros)"
   ]
